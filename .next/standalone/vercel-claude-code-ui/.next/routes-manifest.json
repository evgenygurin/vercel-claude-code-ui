{
  "version": 3,
  "pages404": true,
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    },
    {
      "source": "/home",
      "destination": "/",
      "statusCode": 308,
      "regex": "^(?!/_next)/home(?:/)?$"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ],
      "regex": "^(?:/(.*))(?:/)?$"
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ],
      "regex": "^/api(?:/(.*))(?:/)?$"
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ],
      "regex": "^/_next/static(?:/(.*))(?:/)?$"
    }
  ],
  "rewrites": {
    "beforeFiles": [],
    "afterFiles": [
      {
        "source": "/api/health",
        "destination": "/api/health/route",
        "regex": "^/api/health(?:/)?$"
      }
    ],
    "fallback": []
  },
  "dynamicRoutes": [
    {
      "page": "/api/projects/[id]",
      "regex": "^/api/projects/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/projects/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/api/sessions/[id]",
      "regex": "^/api/sessions/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/sessions/(?<nxtPid>[^/]+?)(?:/)?$"
    },
    {
      "page": "/api/sessions/[id]/messages",
      "regex": "^/api/sessions/([^/]+?)/messages(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/api/sessions/(?<nxtPid>[^/]+?)/messages(?:/)?$"
    },
    {
      "page": "/claude/chat/[id]",
      "regex": "^/claude/chat/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPid": "nxtPid"
      },
      "namedRegex": "^/claude/chat/(?<nxtPid>[^/]+?)(?:/)?$"
    }
  ],
  "staticRoutes": [
    {
      "page": "/",
      "regex": "^/(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/(?:/)?$"
    },
    {
      "page": "/_not-found",
      "regex": "^/_not\\-found(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/_not\\-found(?:/)?$"
    },
    {
      "page": "/analytics",
      "regex": "^/analytics(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/analytics(?:/)?$"
    },
    {
      "page": "/chat",
      "regex": "^/chat(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/chat(?:/)?$"
    },
    {
      "page": "/claude",
      "regex": "^/claude(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/claude(?:/)?$"
    },
    {
      "page": "/editor",
      "regex": "^/editor(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/editor(?:/)?$"
    },
    {
      "page": "/files",
      "regex": "^/files(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/files(?:/)?$"
    },
    {
      "page": "/git",
      "regex": "^/git(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/git(?:/)?$"
    },
    {
      "page": "/integrations",
      "regex": "^/integrations(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/integrations(?:/)?$"
    },
    {
      "page": "/projects",
      "regex": "^/projects(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/projects(?:/)?$"
    },
    {
      "page": "/settings",
      "regex": "^/settings(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/settings(?:/)?$"
    },
    {
      "page": "/team",
      "regex": "^/team(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/team(?:/)?$"
    },
    {
      "page": "/terminal",
      "regex": "^/terminal(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/terminal(?:/)?$"
    }
  ],
  "dataRoutes": [],
  "rsc": {
    "header": "rsc",
    "varyHeader": "rsc, next-router-state-tree, next-router-prefetch, next-router-segment-prefetch",
    "prefetchHeader": "next-router-prefetch",
    "didPostponeHeader": "x-nextjs-postponed",
    "contentTypeHeader": "text/x-component",
    "suffix": ".rsc",
    "prefetchSuffix": ".prefetch.rsc",
    "prefetchSegmentHeader": "next-router-segment-prefetch",
    "prefetchSegmentSuffix": ".segment.rsc",
    "prefetchSegmentDirSuffix": ".segments"
  },
  "rewriteHeaders": {
    "pathHeader": "x-nextjs-rewritten-path",
    "queryHeader": "x-nextjs-rewritten-query"
  }
}